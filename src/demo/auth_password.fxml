<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.LoginController">
   <children>
      <VBox prefHeight="400.0" prefWidth="640.0">
        <children>
          
          <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                  <Label layoutX="119.0" layoutY="32.0" prefHeight="70.0" prefWidth="403.0" text="Waz Authenticator" textFill="#1e1c9a">
                     <font>
                        <Font size="48.0" />
                     </font>
                  </Label>
                  <Label layoutX="221.0" layoutY="110.0" text="Votre mot de passe pour continuer" />
                  <Button fx:id="submit" layoutX="221.0" layoutY="192.0" mnemonicParsing="false" onAction="#processAuth" prefHeight="34.0" prefWidth="186.0" text="Valider" />
                  <PasswordField fx:id="password" layoutX="222.0" layoutY="145.0" prefHeight="34.0" prefWidth="186.0" promptText="Password" />
                  <Hyperlink layoutX="222.0" layoutY="252.0" onAction="#editForm" text="Modifier" />
                  <Hyperlink layoutX="332.0" layoutY="252.0" onAction="#displayInfos" text="Informations" />
            </children>
          </AnchorPane>
            <Label prefHeight="17.0" prefWidth="163.0" text="Copyright M@rdets" />
        </children>
      </VBox>
   </children>
</AnchorPane>
